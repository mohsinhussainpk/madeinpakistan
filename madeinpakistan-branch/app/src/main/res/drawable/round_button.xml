<?xml version="1.0" encoding="utf-8"?>
<selector >


    <item android:state_pressed="false" xmlns:android="http://schemas.android.com/apk/res/android" >
        <shape android:shape="oval"
            android:height="80dp" android:width="80dp"
            >

            <solid android:color="#ffffff"/>




            <stroke android:width="0.5dp" android:color="@color/colorPrimaryDark" />

        </shape>



    </item>
    <item android:state_pressed="true" xmlns:android="http://schemas.android.com/apk/res/android">
        <shape android:shape="oval">

            <solid android:color="#D3D3D3"/>
            <stroke android:width="1.5dp" android:color="#000000" />
        </shape>
    </item>




</selector>